{
	"name": "Burst My Bubble",
	"version": "0.0.1",
	"manifest_version": 2,
	"description": "Burst My Bubble is an extension that aims to pop your filter bubble and give you an unbiased insight into current news.",
	"homepage_url": "http://extensionizr.com",
	"icons": {
		"16": "icons/icon16.png",
		"48": "icons/icon48.png",
		"128": "icons/icon128.png"
	},
	"default_locale": "en",
	"background": {
		"scripts": ["src/bg/background.js"]
	},
	"browser_action": {
		"default_icon": "icons/icon19.png",
		"default_title": "browser action demo",
		"default_popup": "src/browser_action/browser_action.html"
	},
	"permissions": ["cookies", "history", "tabs", "https://*/*"]
}
